@model IEnumerable<WEBFrontQuala.Models.MonMonedum>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Crear Nueva Moneda</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMoneda">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.IdMoneda">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMoneda">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
